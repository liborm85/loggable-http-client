{
    "name": "liborm85/loggable-http-client",
    "description": "Extended logging for the Symfony HTTP Client.",
    "type": "library",
    "keywords": [
        "php",
        "symfony",
        "http-client",
        "http",
        "log",
        "logger",
        "logging",
        "psr-3"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Libor M.",
            "email": "liborm85@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/liborm85/loggable-http-client/issues",
        "source": "https://github.com/liborm85/loggable-http-client"
    },
    "provide": {
        "symfony/http-client-implementation": "3.0",
        "psr/log-implementation": "1.0|2.0|3.0"
    },
    "require": {
        "php": ">=8.1",
        "symfony/http-client-contracts": "^3.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-json": "*",
        "phpunit/phpunit": "^10.0",
        "symfony/http-client": "^6.1",
        "symfony/process": "^6.1",
        "amphp/http-client": "^4.2.1"
    },
    "suggest": {
        "symfony/http-client": "This package requires an actual Symfony HTTP client implementation to decorate."
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Liborm85\\LoggableHttpClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Liborm85\\LoggableHttpClient\\Tests\\": "tests/"
        }
    }
}
